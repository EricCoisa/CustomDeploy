{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "CustomDeploy",
    "Audience": "CustomDeploy",
    "Key": "CustomDeploy-Super-Secret-Key-For-Development-Environment-2024",
    "ExpirationInMinutes": 60
  },
  "DeploySettings": {
    "WorkingDirectory": "C:\\temp\\CustomDeploy",
    "PublicationsPath": "C:\\temp\\wwwroot"
  },
  "GitHubSettings": {
    "Username": "YOUR_GITHUB_USERNAME_HERE",
    "PersonalAccessToken": "YOUR_GITHUB_TOKEN_HERE",
    "UseSystemCredentials": false,
    "GitTimeoutSeconds": 300,
    "ApiBaseUrl": "https://api.github.com"
  }
}
